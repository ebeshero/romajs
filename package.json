{
  "name": "romajs",
  "version": "0.0.1",
  "description": "Javascript implementation of Roma in React-Redux",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "start": "webpack-dashboard -t 'Roma JS' -- node server.js ",
    "build": "rm -rf ./build && NODE_ENV=\"production\" webpack",
    "lint-break-on-errors": "eslint ./source/js ./webpack.config.js -f table --ext .js --ext .jsx",
    "lint": "eslint ./source/js ./webpack.config.js -f table --ext .js --ext .jsx || true",
    "preview": "NODE_ENV=\"production\" ./node_modules/webpack-dashboard/bin/webpack-dashboard.js -t 'Preview Mode - React-Redux Boilerplate' -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raffazizzi/romajs"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "es6-promise": "^3.1.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.0",
    "react-dom": "^15.5.4",
    "react-redux": "^4.4.0",
    "react-router": "^3.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "react-mdl": "^1.9.0",
    "redux-logger": "^2.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.7.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.6.1",
    "cross-env": "^1.0.7",
    "css-loader": "0.14.5",
    "enzyme": "^2.7.1",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "expect": "^1.8.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.9.0",
    "form-data": "^2.1.0",
    "html-webpack-plugin": "^2.24.1",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^5.6.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "prepush": "^3.1.11",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "redux-logger": "^2.7.4",
    "redux-mock-store": "^1.2.1",
    "reselect": "^2.5.4",
    "sass-loader": "^4.0.2",
    "save-as": "^0.1.8",
    "squash-xml-json": "^0.1.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^2.2.1"
  }
}
